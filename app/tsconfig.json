{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "noImplicitAny": false,
    "allowJs": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "skipLibCheck": true
  },
  "include": [
    "./app/**/*.ts",
    "./app/**/*.tsx"
  ]
}
